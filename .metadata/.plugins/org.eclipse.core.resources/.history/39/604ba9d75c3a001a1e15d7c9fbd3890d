package siti.inf203.jena.wordnet;

import org.apache.lucene.wordnet.Syns2Index;

public class ExpandWithWordnet {
	public static String INDEX_DIRECTORY = "synIndex";
	public static String SYN_WORD = "resources/wn_s.pl";
	public static void main(String... args) throws Throwable {
		 Syns2Index.main(new String[] {SYN_WORD , INDEX_DIRECTORY});
		}
}
